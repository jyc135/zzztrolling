{
  "manifest_version": 2,
  "name": "Set Extension",
  "version": "0.1",
  "content_scripts": [
      {
        "matches": [
          "https://setwithfriends.com/game/*"
        ],
        "js": ["content.js"]
      }
  ],
  "browser_action": {
      "default_popup": "options.html"
  },
"permissions": [
    "storage"
  ]
}